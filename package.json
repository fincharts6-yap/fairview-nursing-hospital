  },
  "devDependencies": {
    "@babel/core": "7.22.0",
    "@babel/preset-env": "7.22.0",
    "@babel/preset-react": "7.22.0",
    "babel-loader": "9.1.0",
    "css-loader": "6.8.1",
    "html-webpack-plugin": "5.5.3",
    "style-loader": "3.3.3",
